	"Kafic end"
;

	
Nivo:
	"Nivo"
	naziv_nivoa=STRING ':'
	'Duzina=' Duzina=INT
	'Sirina=' Sirina=INT
	boja_pogloge=Boja
	'Enterijer:'
		element*=Element
	"Kraj Nivoa"	
;
	
Element:
	"Element"
	naziv_elementa=STRING ':'
