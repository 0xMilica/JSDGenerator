	"Kraj Elementa"
;

Oblik:
Linija|Trougao|Pravougaonik|Elipsa
;


Tacka:
	'x=' x=INT ',' 'y=' y=INT
;

	
Linija:
	'Linija:'
	tacka*=Tacka[";"]
;

	
Trougao:
	'Trougao:'
	tacka*=Tacka[";"]
;


Pravougaonik:
	'Pravougaonik:'
